  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "markdownlin.config": {
    "MD041": false
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    "dist": true,
    "out": true,
    "package-lock.json": true,
    "LICENSE": true
  },
  "files.associations": {
    "xlocale": "cpp"
  }
}
